{
  "$schema": "appsettings-schema.json",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information"
    },
    "WriteTo": [
      {
        "Name": "Async",
        "Args": {
          "configure": [
            {
              "Name": "Console"
            }
          ]
        }
      }
    ]
  },
  "Umbraco": {
    "CMS": {
      "Global": {
        "UseHttps": true
      },
        "Content": {
          "MacroErrors": "Throw"
        },
        "Hosting": {
          "Debug": true
        },
        "RuntimeMinification": {
          "UseInMemoryCache": true,
          "CacheBuster": "Timestamp"
        }
      }
    }
}
